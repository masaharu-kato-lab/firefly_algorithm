{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "_init_bench.py",
            "program": "${workspaceFolder}/src/discrete/_init_bench.py",
            "args":[
                "-n", "100"
            ],
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal"
        },
        {
            "name": "_run_opu.py",
            "program": "${workspaceFolder}/src/discrete/_run_opu.py",
            "args":[
                "-n", "30",
                "-g", "0.01502",
                "-a", "2.0",
                "-ba", "2.0",
                "-t", "100",
                "-d", "2",
                "-u", "0",
                "--init", "random",
                "--quiet",
                "--stdout"
            ],
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal"
        },
        {
            "name": "_run_opu.py (simon evaluator)",
            "program": "${workspaceFolder}/src/discrete/_run_opu.py",
            "args":[
                "-n", "30",
                "-g", "0.01502",
                "-a", "2.0",
                "-ba", "2.0",
                "-t", "100",
                "-d", "2",
                "-u", "0",
                "-e", "Simon",
                "-jo", "out/json/output.json",
                "--init", "random",
                "--quiet",
                "--stdout"
            ],
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal"
        },
    ]
}