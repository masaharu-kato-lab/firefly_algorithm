{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "route_planning/_run.py",
            "program": "${workspaceFolder}/src/route_planner/_run.py",
            "args":[
                "--n_run", "10",
                "--n_indiv", "50",
                "--gamma", "0.1",
                "--alpha", "4.0",
                "--blocked_alpha", "4.0",
                "--n_iterate", "200",
                "--n_drones", "4",
                "--init_cls_method", "rm",
                "--init_bld_method", "cpnn",
                "--init_cls_dist"  , "angle",
                "--init_bld_dist"  , "aster",
                "--init_n_cls"     , "4"    ,
            ],
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal"
        }
    ]
}